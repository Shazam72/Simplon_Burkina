var jQueryUrvanovSyntaxHighlighter=jQuery;(function($){UrvanovSyntaxHighlighterUtil=new function(){var base=this;var settings=null;base.init=function(){settings=UrvanovSyntaxHighlighterSyntaxSettings;base.initGET();};base.addPrefixToID=function(id){return id.replace(/^([#.])?(.*)$/,'$1'+settings.prefix+'$2');};base.removePrefixFromID=function(id){var re=new RegExp('^[#.]?'+settings.prefix,'i');return id.replace(re,'');};base.cssElem=function(id){return $(base.addPrefixToID(id));};base.setDefault=function(v,d){return(typeof v=='undefined')?d:v;};base.setMax=function(v,max){return v<=max?v:max;};base.setMin=function(v,min){return v>=min?v:min;};base.setRange=function(v,min,max){return base.setMax(base.setMin(v,min),max);};base.getExt=function(str){if(str.indexOf('.')==-1){return undefined;}
